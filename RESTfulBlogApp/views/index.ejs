<%- include("partials/header") %>
<div class="container">
  <h1>
    Index page
  </h1>
  <%blogs.forEach((blog)=>{%>
  <div>
    <h2><%= blog.title%></h2>
    <img src="<%=blog.image%>" />
    <p><%= blog.body%></p>
    <a href="/blogs/<%= blog._id%>">Read more</a>
  </div>
</div>
<%});%> <%- include("partials/footer") %>
